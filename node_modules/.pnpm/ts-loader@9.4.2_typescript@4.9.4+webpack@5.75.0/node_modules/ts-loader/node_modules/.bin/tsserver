#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/node_modules:/Users/warryy/code/typescript/node_modules:/Users/warryy/code/node_modules:/Users/warryy/node_modules:/Users/node_modules:/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/ts-loader@9.4.2_typescript@4.9.4+webpack@5.75.0/node_modules/ts-loader/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/ts-loader@9.4.2_typescript@4.9.4+webpack@5.75.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/bin/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules/typescript/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/typescript@4.9.4/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/node_modules:/Users/warryy/code/typescript/node_modules:/Users/warryy/code/node_modules:/Users/warryy/node_modules:/Users/node_modules:/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/ts-loader@9.4.2_typescript@4.9.4+webpack@5.75.0/node_modules/ts-loader/node_modules:/Users/warryy/code/typescript/node_modules/.pnpm/ts-loader@9.4.2_typescript@4.9.4+webpack@5.75.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.9.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.9.4/node_modules/typescript/bin/tsserver" "$@"
fi
